<div class="container">
  <h1>Edit Your Profile</h1>
<form method="post" action="editProfile">
<div class="row">
  <div class="col-sm-6">
  </div>

    <div class="col-sm-6">
        <table class="table table-bordered">
          <tbody>
            <tr>
              <td>googlename</td>
              <td><%= user.googlename %></td>
            </tr>
            <tr>
              <td>googleemail</td>
              <td><%= user.googleemail %></td>
            </tr>
            <tr>
              <td>Preferred Name</td>
              <td>
                <input type="text" name="username"
                   value="<%= user.username %>">
              </td>
            </tr>
            <tr>
              <td>age</td>
              <td>
                <input type="text" name="age"
                   value="<%= user.age %>">
              </td>
            </tr>
            <tr>
              <td>imageURL</td>
              <td>
                <input type="text" name="imageURL"
                   value="<%= user.imageURL %>">
              </td>
            </tr>
            <tr>
              <td>Phone Number</td>
              <td>
                <input type="text" name="phone"
                   value="<%= user.phone %>">
              </td>
            </tr>
            <tr>
              <td>Address</td>
              <td>
                <input type="text" name="address"
                   value="<%= user.address %>">
              </td>
            </tr>
          </tbody>
        </table>
    </div>


</div>
<input class="btn btn-lg btn-danger" type="submit" value="update Profile">
</form>
</div>
